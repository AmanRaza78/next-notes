generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id        String     @id
  firstName String
  lastName  String
  email     String
  stripeCustomerId String? @unique
  colorScheme String @default("theme-orange")
  createdAt DateTime   @default(now())
}


